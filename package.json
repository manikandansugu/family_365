{
  "name": "family365",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "android-clean": "cd android && ./gradlew clean && cd ..",
    "open-debug-apk": "open ./android/app/build/outputs/apk/debug/",
    "android-debug-apk": "cd android && gradlew assembleDebug && cd .. && npm run open-debug-apk",
    "open-apk": "open ./android/app/build/outputs/apk/release/",
    "android-apk": "cd android && gradlew assembleRelease && cd .. && npm run open-apk",
    "open-aab": "open ./android/app/build/outputs/bundle/release/",
    "android-aab": "cd android && gradlew bundleRelease && cd .. && npm run open-aab",
    "android-sha-debug": "keytool -list -v -keystore debug.keystore -alias androiddebugkey -storepass android -keypass android",
    "android-sha-release": "keytool -list -v -keystore my-release-key.keystore",
    "android-keystore-debug": "keytool -genkeypair -v -keystore debug.keystore -storepass android -keypass android -alias androiddebugkey -keyalg RSA -keysize 2048 -validity 10000 -dname 'CN=Android Debug,O=Android,C=US'",
    "android-keystore-release": "keytool -genkeypair -v -keystore release.keystore -alias your-key-alias -keyalg RSA -keysize 2048 -validity 10000",
    "bundle": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-native-community/datetimepicker": "^8.2.0",
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@react-native/gradle-plugin": "^0.77.0",
    "@react-navigation/bottom-tabs": "^7.1.3",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/stack": "^7.1.1",
    "@reduxjs/toolkit": "^2.3.0",
    "axios": "^1.7.8",
    "buffer": "^6.0.3",
    "i18next": "^23.16.5",
    "lottie-react-native": "^7.1.0",
    "moment": "^2.30.1",
    "react": "18.3.1",
    "react-i18next": "^15.1.1",
    "react-native": "0.76.1",
    "react-native-collapsible": "^1.6.2",
    "react-native-date-picker": "^5.0.7",
    "react-native-gesture-handler": "^2.22.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-image-picker": "^7.2.2",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "^3.16.7",
    "react-native-reanimated-carousel": "^3.5.1",
    "react-native-safe-area-context": "^5.2.0",
    "react-native-screens": "^4.7.0",
    "react-native-svg": "^15.8.0",
    "react-native-vector-icons": "^10.2.0",
    "react-redux": "^9.1.2",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.0",
    "@react-native-community/cli-platform-android": "15.0.0",
    "@react-native-community/cli-platform-ios": "15.0.0",
    "@react-native/babel-preset": "0.76.1",
    "@react-native/eslint-config": "0.76.1",
    "@react-native/metro-config": "0.76.1",
    "@react-native/typescript-config": "0.76.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
